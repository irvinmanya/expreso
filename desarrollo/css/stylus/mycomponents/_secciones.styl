@import 'nib'

vendor-prefixes = webkit moz ms o official
scale = 0 480px 768px 992px 1200px

section
	tam ()


//------------> [ Intro ] <-----------------//
.intro
	tam ()
	padding 20px 0px 0px 0px
	
.sliderIntro
	tam ()
	font-size 0px
	

.sliderFeat
	tam ()
	width calc( 100% - 235px )
	max-height 406px
	overflow hidden
	li
		tam ()
		figure
			tam ()
			max-height 406px
			overflow hidden
			overflow hidden
			figcaption
				position absolute
				width 100%
				height auto
				bottom 0px
				left 0px
				background -moz-linear-gradient(top, rgba(0,0,0,0) 0%, rgba(0,0,0,1) 86%, rgba(0,0,0,1) 100%)
				background -webkit-linear-gradient(top, rgba(0,0,0,0) 0%,rgba(0,0,0,1) 86%,rgba(0,0,0,1) 100%)
				background linear-gradient(to bottom, rgba(0,0,0,0) 0%,rgba(0,0,0,1) 86%,rgba(0,0,0,1) 100%)
				text-align left
				z-index 1
				padding 5px 10px 5px 10px
				&:before
					befcont ()
					bottom 0px
					left 0px
					background rgba(0,0,0,.5)
				h2
				p
					tam ()
					color blanco
					padding 0px 0px 7px 0px
				a
					color blanco
					&:hover
					&:active
						color blanco
	
.sliderList
	tam ()
	width 235px
	max-height 406px
	overflow hidden
	li
		tam ()
		height auto
		min-height 100px
		max-height 100px
		overflow hidden
		border-bottom 1px solid rgba(0,0,0,.15)
		font-size 0px
		trancicion ()
		display table
		&:before
			befcont ()
			width 10px
			height 10px
			top 0px
			bottom 0px
			left -7px
			margin auto
			transform rotate(45deg)
			background negro
			opacity 0
			img
				position absolute
				top 0px
				left 0px
				right 0px
				margin-left auto
				margin-right auto
				width auto
				height 100%
		.sliderTxt
			tam ()
			display table-cell
			vertical-align middle
			width 100%
			text-align left
			padding 5px 7px 5px 15px
			h3
				tam ()
				font-size 18px
		&:last-child
			border-bottom 0px solid blanco
		&:hover
		&:active
		&:focus
			border-left 3px solid negro
			&:before
				opacity 1

.sliderFeatHide
	display none !important
.sliderFeatAct
	display inline-block !important

// .sliderNav
